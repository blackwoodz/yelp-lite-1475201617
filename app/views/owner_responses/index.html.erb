<div class="page-header">
  <h1>
    Owner responses
    <a href="/owner_responses/new" class="btn btn-lg btn-success">New Owner response</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Response</th>
        <th>Review</th>
        <th>Ownership</th>
        <th>Actions</th>
      </tr>

      <% @owner_responses.each do |owner_response| %>
      <tr>
        <td><%= owner_response.response %></td>
        <td><%= owner_response.review_id %></td>
        <td><%= owner_response.ownership_id %></td>
        <td>
          <a href="/owner_responses/<%= owner_response.id %>" class="btn btn-primary">Show</a>
          <a href="/owner_responses/<%= owner_response.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_owner_response/<%= owner_response.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
